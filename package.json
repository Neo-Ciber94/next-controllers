{
  "name": "next-controllers",
  "description": "An api for create routing controllers in nextjs",
  "version": "0.1.9",
  "license": "MIT",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "keywords": [
    "nextjs",
    "api",
    "router",
    "routing",
    "controllers",
    "typescript"
  ],
  "scripts": {
    "lint": "eslint --ext .ts,.js src",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "publish": "tsc && npm publish",
    "watch": "npm-watch",
    "compile": "tsc"
  },
  "watch": {
    "compile": {
      "patterns": [
        [
          "src"
        ]
      ],
      "extensions": "ts,js",
      "quiet": true
    }
  },
  "dependencies": {
    "next": "12.0.4",
    "regexparam": "^2.0.0",
    "stacktrace-parser": "^0.1.10"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "eslint": "^7.32.0",
    "npm-watch": "^0.11.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^8.2.2",
    "typescript": "^4.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Neo-Ciber94/next-controllers.git"
  }
}
