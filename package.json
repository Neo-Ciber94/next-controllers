{
  "name": "next-controllers",
  "description": "An api for create routing controllers in nextjs",
  "version": "0.1.11",
  "license": "MIT",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "keywords": [
    "nextjs",
    "api",
    "router",
    "routing",
    "controllers",
    "typescript"
  ],
  "scripts": {
    "lint": "eslint --ext .ts,.js src",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "publish": "npm run build:prod && npm publish",
    "test": "jest",
    "watch": "npm-watch",
    "build:dev": "tsc",
    "build:prod": "npm run clean && tsc -p tsconfig.prod.json",
    "clean": "rm -r ./build"
  },
  "watch": {
    "build:dev": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "ts,js",
      "quite": false
    }
  },
  "dependencies": {
    "next": "^12.0.7",
    "regexparam": "^2.0.0",
    "stacktrace-parser": "^0.1.10"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.5",
    "@types/jest": "^27.0.3",
    "@types/node": "16.11.7",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "eslint": "^7.32.0",
    "jest": "^27.4.5",
    "npm-watch": "^0.11.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^8.2.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Neo-Ciber94/next-controllers.git"
  }
}
